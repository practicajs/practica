{
  "files.exclude": {
    "**/.dist": true
  },
  "cSpell.words": ["practica"],
  "editor.suggest.showStatusBar": true,
  "debug.showInStatusBar": "onFirstSessionStart",
  "jestrunner.debugOptions": {
    "skipFiles": [
      "${workspaceFolder}/node_modules/**/*.js",
      "<node_internals>/**",
      "**/node_modules/*.*"
    ],
    "smartStep": true
  },
  "typescript.tsdk": "node_modules/typescript/lib"
}
